<h1>Manage Coupons</h1>

<table>
  <tr>
	<th>Coupon ID</th>
    <th>Coupon type</th>
    <th>Client user</th>
    <th>Client comment</th>
    <th>Created at</th>
    <th>Used date</th>
    <th>Consuming partner</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @coupons.each do |coupon| %>
  <tr>
	<td><%= coupon.id %></td>
    <td><%= coupon.coupon_type_id %></td>
    <td><%= coupon.client_user %></td>
    <td><%= coupon.client_comment %></td>
    <td><%= coupon.created_at %></td>
    <td><%= coupon.used_date %></td>
    <td><%= coupon.consuming_partner %></td>
    <td><%= link_to 'Show', coupon %></td>
    <td><%= link_to 'Validate', edit_coupon_path(coupon) %></td>
    <td><%= link_to 'Destroy', coupon, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Generate Coupon', new_coupon_path %>
